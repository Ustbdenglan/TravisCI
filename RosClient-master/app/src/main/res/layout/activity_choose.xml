<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="net.xxhong.rosclient.ui.ChooseActivity">

    <Button
        android:id="@+id/btn_move_base"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="122dp"
        android:onClick="onMoveBaseClick"
        android:text="chassis"
        tools:layout_editor_absoluteX="153dp"
        tools:layout_editor_absoluteY="66dp" />

    <Button
        android:id="@+id/btn_head"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_move_base"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="125dp"
        android:onClick="onHeadClick"
        android:text="head"
        tools:layout_editor_absoluteX="153dp"
        tools:layout_editor_absoluteY="153dp" />

</RelativeLayout>
